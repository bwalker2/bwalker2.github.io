function Signup(){var n=".iu-feature-subscription-signup";this.init=function(){if(!(jQuery(n).length<1))jQuery(".newsletter-subscription-form",n).on("submit",function(t){t.preventDefault();jQuery(".hidden-message",n).hide();var i=jQuery(this),r=i.find("[id*=email]").val(),u=i.find("[id*=goal]").val();jQuery.ajax({url:window.location.href,type:"POST",data:{scAction:"NewsletterSignUp",scController:"Subscription",emailAddress:r,goalId:u},context:i,success:function(n){n.success?(i.find(".success-message").show(),i.find("[id*=email]").parent().hide()):i.find(".failure-message").show()},error:function(){i.find(".failure-message").show()}})})}}jQuery(document).ready(function(){(new Signup).init()});